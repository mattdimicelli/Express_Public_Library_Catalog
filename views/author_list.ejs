<%- include('partials/_top') %>

<title><%= title %></title>

<%- include('partials/_middle') %>

<h1><%= title %> </h1>

<ul>
    <% if (author_list.length > 0) { %>
        <% author_list.forEach(author => { %>
            <li>
                <a href="<%= author.url %>"><%= author.name %> </a>
                <%= author.lifespan %>  
            </li>
            <% }) %>
    <% } else { %>
        <li>There are no authors</li>
    <% } %>
</ul>

<%- include('partials/_bottom') %>